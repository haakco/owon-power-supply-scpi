*.t3d binary filter=lfs diff=lfs merge=lfs -text
*.exe binary filter=lfs diff=lfs merge=lfs -text
*.data binary filter=lfs diff=lfs merge=lfs -text
*.ttf binary filter=lfs diff=lfs merge=lfs -text
*.eof binary filter=lfs diff=lfs merge=lfs -text
*.eot binary filter=lfs diff=lfs merge=lfs -text
*.swf binary filter=lfs diff=lfs merge=lfs -text
*.mov binary filter=lfs diff=lfs merge=lfs -text
*.mp4 binary filter=lfs diff=lfs merge=lfs -text
*.mp3 binary filter=lfs diff=lfs merge=lfs -text
*.ogg binary filter=lfs diff=lfs merge=lfs -text
*.flv binary filter=lfs diff=lfs merge=lfs -text
